#: src/euphorie/client/templates/error.pt:15
msgid "title_error"
msgstr "Kļūda"

#. Default: We're sorry, but an unforseen error has occured.
#: src/euphorie/client/templates/error.pt:17
msgid "expl_error"
msgstr "Ir radusies darbības kļūda."

#. Default: Not found
#: src/euphorie/client/templates/error_notfound.pt:15
